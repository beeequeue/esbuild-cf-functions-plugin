{
  "name": "esbuild-cf-functions-plugin",
  "type": "module",
  "description": "A plugin to configure ESBuild for building code compatible with CloudFront Functions",
  "version": "1.1.2",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/beeequeue/esbuild-cf-functions-plugin.git"
  },
  "homepage": "https://github.com/BeeeQueue/esbuild-cf-functions-plugin",
  "keywords": [
    "aws",
    "cloudfront",
    "functions",
    "function",
    "lambda",
    "esbuild",
    "compiler",
    "bundler",
    "plugin"
  ],
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "main": "./dist/plugin.cjs",
  "module": "./dist/plugin.js",
  "types": "./dist/plugin.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/plugin.d.cts",
        "default": "./dist/plugin.cjs"
      },
      "import": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      }
    },
    "./package.json": "./package.json"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src example/src",
    "test": "vitest run",
    "test:dev": "vitest",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "release": "pnpm build && pnpm changeset publish",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "esbuild": ">=0.14.46"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.3.0",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.28.1",
    "@tsconfig/node18": "18.2.4",
    "@types/node": "22.13.14",
    "@vitest/coverage-v8": "3.1.1",
    "dotenv": "16.4.7",
    "eslint": "9.21.0",
    "lint-staged": "16.1.0",
    "nanoid": "5.1.5",
    "prettier": "4.0.0-alpha.12",
    "simple-git-hooks": "2.12.1",
    "ts-dedent": "2.2.0",
    "tsup": "8.4.0",
    "typescript": "5.8.2",
    "vitest": "3.1.1"
  },
  "pnpm": {
    "overrides": {
      "@eslint/markdown": "-",
      "@stylistic/eslint-plugin": "-",
      "eslint-plugin-jsonc": "-",
      "eslint-plugin-toml": "-",
      "eslint-plugin-vue": "-",
      "eslint-processor-vue-blocks": "-",
      "eslint-plugin-yml": "-",
      "jsonc-eslint-parser": "-",
      "toml-eslint-parser": "-",
      "yaml-eslint-parser": "-",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1"
    },
    "ignoredBuiltDependencies": [
      "simple-git-hooks"
    ],
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}

{
  "name": "@beequeue/project-template",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@7.2.0",
  "engines": {
    "node": ">=16.15"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "dev",
    "build": "node build.mjs",
    "lint": "eslint src",
    "test": "vitest run",
    "test:dev": "vitest",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "0.6.0",
    "@tsconfig/node16": "1.0.2",
    "c8": "7.11.3",
    "dotenv": "16.0.1",
    "esbuild": "0.14.46",
    "eslint": "8.17.0",
    "lint-staged": "13.0.1",
    "prettier": "2.6.2",
    "simple-git-hooks": "2.8.0",
    "typescript": "4.7.3",
    "vitest": "0.14.2"
  }
}
